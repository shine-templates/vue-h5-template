{
    "env": {
        "node": true,
        "browser": true,
        "commonjs": true,
        "amd": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-essential"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-console": "off",
        "no-debugger": "warn",
        "no-unused-components": "off",
        "no-unused-vars": "off",
        "no-empty-function": "off",
        "prefer-const": "off",
        "vue/multi-word-component-names": "off"
    }
}